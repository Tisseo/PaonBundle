{% form_theme form 'TisseoTidBundle:Form:fields.html.twig' %}
<div class="row">
    <div class="col-md-12">
        {{ form_start(form) }}

        {{ form_row(form.date) }}
        {{ form_row(form.comment) }}
        {{ form_row(form.file) }}
        <input type="submit" class="btn btn-success clearfix pull-right" value="{{ 'line_schema.labels.submit_file'|trans({}, 'messages') }}" >
        {{ form_end(form) }}

    </div>
</div>
