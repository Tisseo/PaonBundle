{% extends "TisseoCoreBundle::generic_list.html.twig" %}
{% set bundle = "TisseoPaonBundle" %}

{% block table_head -%}
    <th class="col-md-10">{{ 'tisseo.paon.data_exchange.label.job'|trans }}</th>
    <th class="col-md-2">{{ 'tisseo.global.actions'|trans }}</th>
{% endblock %}

{% block breadcrumb %}
    {{ parent() }}
    {% image '@TisseoCoreBundle/Resources/public/images/ajax-loader.gif' %}
        <img src="{{ asset_url }}" id="loading-indicator" style="display:none;"/>
    {% endimage %}
{% endblock %}

{% block table_classes %}table-hover table-jobs{% endblock %}
{% block table_body -%}
    {{ render(controller('TisseoPaonBundle:DataExchange:jobs')) }}
{% endblock %}

{% block content %}
    {{ parent() }}
    {{ render(controller('TisseoPaonBundle:DataExchange:lines')) }}
{% endblock %}

{% block javascripts %}
    <script>
        require(['paon/list/dataexchange']);
        require(['paon/importfh/dataexchange']);
    </script>
{% endblock %}
